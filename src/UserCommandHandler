UserCommandHandler() {
  case "$1" in
    create     ) SynapseUserCreate;;
    password   ) SynapseUserResetPassword;;
    deactivate ) SynapseUserDeactivate;;
    shadowban  ) SynapseUserShadowban;;
    ''         ) Log error "Subcommand required. Type 'help user' for a list of subcommands.";;
    *          ) Log error "Unrecognized subcommand '$1'. Type 'help user' for a list of subcommands.";;
  esac
}
