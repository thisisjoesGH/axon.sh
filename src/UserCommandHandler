UserCommandHandler() {
  case "$@" in
    create     ) SynapseUserCreate;;
    password   ) SynapseUserResetPassword;;
    deactivate ) SynapseUserDeactivate;;
    shadowban  ) SynapseUserShadowban;;
    ''         ) Log error "Subcommand required.";;
    *          ) Log error "Unrecognized subcommand '$@'.";;
  esac
}
