  )

  s_purge_history_body_params=(
    [delete_local_events]="false"
    [purge_up_to_ts]=""
  )

  s_delete_room_url_params=(
    [room_id]=""
  )

  s_delete_room_body_params=(
    [block]="false"
    [purge]="false"
  )

  s_user_query_url_params=(
    [user_id]=""
  )

  s_user_joined_rooms_url_params=(
    [user_id]=""
  )

  s_user_modify_body_params=(
