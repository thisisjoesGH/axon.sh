SynapseVersion() {
  Log debug "Got args '$*'"

  GetVersion() {
    local -A params=(
      [method]="${s_version_api[method]}"
      [path]="${s_version_api[path]}"
      [keys]="server_version python_version"
    )
    Request params || return 1

    echo "Server version: ${key_values[server_version]}"
    echo "Python version: ${key_values[python_version]}"
 }

  CheckLoginStatus || return 1

  if [[ ! -z "${config[homeserver]}" ]]; then
    DefineSynapseEndpoints
    GetVersion
  else
    Log error "Failed to get server version. No homeserver is configured."
  fi
}
